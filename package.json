{
  "name": "learn-type-script",
  "version": "1.0.0",
  "description": "learn-type-script-by-thanhly",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "rm -rf ./dist/* && npm run build-ts && npm run tslint",
    "watch-node": "nodemon ./dist/index.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "mocha --recursive -r ts-node/register **/test/**/*.test.ts",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha --recursive -r ts-node/register **/test/**/*.test.ts\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/method-override": "0.0.29",
    "@types/mocha": "^8.2.0",
    "@types/mongoose": "^4.7.6",
    "accesscontrol": "^2.2.1",
    "bcrypt": "5.0.0",
    "body-parse": "^0.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "1.4.0",
    "jsonwebtoken": "8.5.1",
    "mocha": "^7.1.2",
    "moment": "2.26.0",
    "mongoose": "5.9.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "2.0.3",
    "ts-node": "^9.1.1",
    "typescript": "3.8.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0"
  }
}
